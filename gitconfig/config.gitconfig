[include]
	path = ~/.dotfiles/gitconfig/host
[pull]
	ff = only
[alias]
	what = log --oneline -n 10
	hsup = pull
	hist = log --follow -p --
	fix = pull --no-ff
[advice]
	statusHints = off
[color]
	ui = auto
[color "diff"]
	meta = white
	frag = cyan
	old = red
	new = green
[color "status"]
	added = green
	changed = yellow
	untracked = red